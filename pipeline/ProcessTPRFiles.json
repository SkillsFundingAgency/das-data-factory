											"source": {
												"type": "AzureSqlSource",
												"sqlReaderQuery": "SELECT '1'",
												"queryTimeout": "02:00:00",
												"partitionOption": "None"
											},
											"dataset": {
												"referenceName": "DestTPR",
												"type": "DatasetReference",
												"parameters": {
													"TableName": "log_execution_results",
													"SchemaName": "mgmt"
												}
											}
										}
