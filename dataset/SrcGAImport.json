{
	"name": "SrcGAImport",
	"properties": {
		"linkedServiceName": {
			"referenceName": "googlecloud_connection",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ContainerName": {
				"type": "string"
			},
			"FileName": {
				"type": "string",
				"defaultValue": "output-00000-of-00001.parquet"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "GoogleCloudStorageLocation",
				"bucketName": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				},
				"folderPath": "gadata",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}