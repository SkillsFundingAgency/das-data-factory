{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "das-at-dfac-df"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://das-at-dfac-kv.vault.azure.net/"
		},
		"avmsplus_dbconnectionstring_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "avmsplus-dbconnectionstring"
		},
		"datamgmt_staging_dbconnectionstring_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "datamgmt-staging-dbconnectionstring"
		},
		"faa_cdbconnectionstring_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "faa-cdbconnectionstring"
		},
		"faa_cdbconnectionstring_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "findapprenticeship"
		},
		"rcrt_cdbconnectionstring_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "rcrt-cdbconnectionstring"
		},
		"rcrt_cdbconnectionstring_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "recruit"
		},
		"Trigger-BuildDataMart_properties_Master-BuildDataMart_parameters_RunId": {
			"type": "string",
			"defaultValue": "\"1\""
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"avmsplus_dbconnectionstring_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('avmsplus_dbconnectionstring_properties_typeProperties_connectionString_secretName')]"
					},
					"datamgmt_staging_dbconnectionstring_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('datamgmt_staging_dbconnectionstring_properties_typeProperties_connectionString_secretName')]"
					},
					"faa_cdbconnectionstring_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('faa_cdbconnectionstring_properties_typeProperties_connectionString_secretName')]"
					},
					"faa_cdbconnectionstring_properties_typeProperties_database": {
						"value": "[parameters('faa_cdbconnectionstring_properties_typeProperties_database')]"
					},
					"rcrt_cdbconnectionstring_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('rcrt_cdbconnectionstring_properties_typeProperties_connectionString_secretName')]"
					},
					"rcrt_cdbconnectionstring_properties_typeProperties_database": {
						"value": "[parameters('rcrt_cdbconnectionstring_properties_typeProperties_database')]"
					},
					"Trigger-BuildDataMart_properties_Master-BuildDataMart_parameters_RunId": {
						"value": "[parameters('Trigger-BuildDataMart_properties_Master-BuildDataMart_parameters_RunId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}